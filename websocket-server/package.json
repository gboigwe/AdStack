{
  "name": "adstack-websocket-server",
  "version": "1.0.0",
  "description": "Real-time WebSocket analytics server for AdStack blockchain advertising platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "nodemon --watch src --exec ts-node src/index.ts",
    "test": "jest",
    "test:load": "node tests/load-test.js",
    "test:stress": "node tests/stress-test.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "keywords": [
    "websocket",
    "socket.io",
    "real-time",
    "analytics",
    "stacks",
    "blockchain",
    "advertising",
    "real-time-analytics"
  ],
  "author": "AdStack Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gboigwe/AdStack"
  },
  "bugs": {
    "url": "https://github.com/gboigwe/AdStack/issues"
  },
  "homepage": "https://github.com/gboigwe/AdStack#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@stacks/blockchain-api-client": "^7.8.0",
    "@stacks/stacks-blockchain-api-types": "^7.8.0",
    "socket.io": "^4.6.1",
    "express": "^4.18.2",
    "redis": "^4.6.5",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.10"
  }
}
